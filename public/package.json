        "@vaadin/router": "^2.0.0",
        "lit": "^3.2.1",
        "marked": "^15.0.7",
        "moment": "^2.29.4",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/node": "^22.13.4",
        "concurrently": "^9.1.2",
        "eslint": "^9.20.1",
        "eslint-plugin-html": "^8.1.2",
        "npm-run-all2": "^7.0.2",
